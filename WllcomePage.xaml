<Window x:Class="Lab1_DesktopAppDev.WllcomePage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab1_DesktopAppDev"
        mc:Ignorable="d"
        Title="WllcomePage" Height="450" Width="800">
    <Grid>
        <RadioButton x:Name="bankAppRadio" Content="Bank Application" HorizontalAlignment="Left" Margin="261,174,0,0" VerticalAlignment="Top" FontSize="14"/>
        <RadioButton x:Name="shippingRadio" Content="Fast Freight Shipping" HorizontalAlignment="Left" Margin="261,207,0,0" VerticalAlignment="Top" FontSize="14"/>
        <RadioButton x:Name="PopulationRadio" Content="Population of organisms prediction" HorizontalAlignment="Left" Margin="261,237,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Label x:Name="chooseAppLabel" Content="Choose Application to run:" HorizontalAlignment="Left" Margin="261,123,0,0" VerticalAlignment="Top" FontSize="14" FontWeight="Bold"/>
        <Button x:Name="runButton" Content="RUN" HorizontalAlignment="Left" Margin="274,281,0,0" VerticalAlignment="Top" Width="40"/>

    </Grid>
</Window>
